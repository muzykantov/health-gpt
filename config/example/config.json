{
  "llm": {
    "activeProvider": "gigachat",
    "gigaChat": {
      "clientId": "your-gigachat-client-id",
      "clientSecret": "your-gigachat-client-secret",
      "model": "GigaChat-Max",
      "temperature": 0.1,
      "topP": 1.0,
      "maxTokens": 2048,
      "repetitionPenalty": 1.0
    },
    "chatGPT": {
      "apiKey": "your-openai-api-key",
      "model": "gpt-4o",
      "temperature": 0.1,
      "topP": 1.0,
      "maxTokens": 2048,
      "socksProxy": ""
    }
  },
  "telegram": {
    "token": "your-telegram-bot-token",
    "debug": true
  },
  "storage": {
    "postgres": {
      "host": "postgres",
      "port": 5432,
      "user": "health_gpt",
      "password": "health_gpt_password",
      "database": "health_gpt",
      "sslMode": "disable"
    },
    "history": {
      "type": "postgres",
      "path": ""
    },
    "users": {
      "type": "file",
      "path": "./data/users"
    }
  }
}